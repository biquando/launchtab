SHELL = /bin/bash

[com.example.task]
echo "It is $(date)!" >>"$FILE"

# run every hour
Interval 3600

# run at 5:30 on mondays
30 5 * * 1

# set environment variables
FILE = /tmp/launchtab-example

# redirect stdin, stdout, and stderr
<  /etc/passwd
>  /dev/null
2> /dev/null

# additional keys
---
<key>RunAtLoad</key>
<true/>
---
