SHELL = /bin/bash

[com.example.task]
echo "It is $(date)!" >>"$FILE"

# run every hour
Interval 3600

# set crontab-style calendar intervals
0    5 * * 1-5
0,30 4 * * */3

# set environment variables
FILE = /tmp/launchtab-example

# redirect stdin, stdout, and stderr
<  /etc/passwd
>  /dev/null
2> /dev/null

# additional keys
---
<key>RunAtLoad</key>
<true/>
---
